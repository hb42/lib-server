{
  "name": "@hb42/lib-server",
  "version": "1.0.5",
  "copyright": "Copyright (c) 2018-2019 Hartmut Bauer",
  "description": "Common objects for node serverss",
  "main": "lib/index.js",
  "typings": "./lib",
  "scripts": {
    "build:prep": "npm --no-git-tag-version version patch",
    "clean:lib": "./node_modules/.bin/rimraf lib",
    "clean:tmp": "./node_modules/.bin/rimraf tmp",
    "clean": "npm run clean:lib && npm run clean:tmp",
    "build": "npm run clean && tsc",
    "commit": "git add -A ./lib && git commit -m \"package\" ./lib",
    "make": "npm run build && npm run commit && npm version prerelease --preid=beta",
    "release:patch": "npm run build && npm run commit && npm version patch",
    "release:minor": "npm run build && npm run commit && npm version minor",
    "release:major": "npm run build && npm run commit && npm version major",
    "release": "npm run release:patch"
  },
  "author": "hb",
  "license": "MIT",
  "repository": "hb42/lib-server",
  "config": {
    "platform": "win"
  },
  "peerDependencies": {
    "@hb42/lib-common": "https://github.com/hb42/lib-common.git#semver:^1.0.0",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.1",
    "express": "^4.14.0",
    "express-session": "^1.14.1",
    "http": "^0.0.0",
    "jsonwebtoken": "^8.0.1",
    "ldapjs": "^1.0.2",
    "log4js": "^4.0.0",
    "mongoose": "^5.0.0",
    "serve-favicon": "^2.3.0",
    "uuid": "^3.0.0"
  },
  "devDependencies": {
    "@hb42/lib-common": "git+https://github.com/hb42/lib-common.git#semver:^1.0.0",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "0.0.36",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.1",
    "@types/express-session": "^1.15.11",
    "@types/jsonwebtoken": "^8.0.0",
    "@types/ldapjs": "^1.0.3",
    "@types/mongoose": "^5.3.12",
    "@types/mysql": "^2.15.5",
    "@types/node": "^10.12.21",
    "@types/serve-favicon": "^2.2.30",
    "@types/uuid": "^3.4.4",
    "angular2-template-loader": "^0.6.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "copyfiles": "^2.1.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "http": "^0.0.0",
    "jsonwebtoken": "^8.4.0",
    "ldapjs": "^1.0.2",
    "log4js": "^4.0.0",
    "mongoose": "^5.4.8",
    "rimraf": "^2.6.3",
    "serve-favicon": "^2.5.0",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "uuid": "^3.3.2"
  },
  "dependencies": {}
}
